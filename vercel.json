{
    "functions": {
		"migrate.sh": { "runtime": "vercel-bash@4.1.0" }
	},
    "builds": [{
            "src": "BaKet/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.4" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "BaKet/wsgi.py"
        }
    ]
}
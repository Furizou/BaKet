{
    "builds": [{
        "src": "BaKet/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb", 
            "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "BaKet/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        }
    ],
    "functions": {
        "*.py": {
            "maxDuration": 60
        }
    }
}